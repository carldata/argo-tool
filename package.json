{
  "name": "argo-tool",
  "version": "0.5.0",
  "description": "argo-tool",
  "scripts": {
    "dev": "webpack-dev-server --config webpack/dev.config.js --mode development --open",
    "dev-mock": "webpack-dev-server --config webpack/dev-mock.config.js --mode development --open",
    "prod-k8s": "webpack --mode production --config webpack/prod.k8s.config.js",
    "prod-fw": "webpack --mode production --config webpack/prod.fw.config.js",
    "analyze": "webpack --mode production --config webpack/analyze.config.js",
    "test": "jest",
    "bd-server-build": "tsc -p tsconfig-server.json && npm run bd-server-copy-static-assets",
    "bd-server-serve": "cd backless-development && node index.js",
    "bd-server-copy-static-assets": "ts-node ./src/backless-dev-server/copy-static-assets.ts"
  },
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jest-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/unit/**.(ts|tsx|js)",
      "**/__tests__/schemas/**.(ts|tsx|js)"
    ],
    "preset": "jest-puppeteer-preset"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bootstrap": "4.0.0",
    "css-loader": "^0.28.4",
    "d3": "^4.7.2",
    "date-fns": "^1.28.5",
    "express": "^4.16.3",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-mathjax": "0.1.1",
    "react-redux": "^5.0.5",
    "react-responsive-modal": "2.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.0",
    "redux-saga": "0.16.0",
    "style-loader": "^0.18.2",
    "time-series-scroller": "0.5.33",
    "yup": "0.25.1"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/js-cookie": "2.1.0",
    "@types/lodash": "4.14.105",
    "@types/node": "9.4.7",
    "@types/puppeteer": "1.2.0",
    "@types/react": "^15.0.26",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.43",
    "@types/react-router": "^4.0.27",
    "@types/react-router-dom": "^4.2.7",
    "@types/redux-saga": "0.10.5",
    "@types/yup": "0.24.4",
    "copy-webpack-plugin": "4.5.1",
    "html-webpack-plugin": "3.0.7",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.8.2",
    "ts-loader": "4.1.0",
    "ts-node": "^7.0.0",
    "tslint": "5.9.1",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "1.2.4",
    "url-loader": "^0.5.9",
    "webpack": "4.1.1",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.0.12",
    "webpack-dev-server": "3.1.1",
    "webpack-merge": "4.1.2"
  }
}
